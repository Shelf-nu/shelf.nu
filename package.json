    "prisma": "^5.4.2",
    "remix-flat-routes": "^0.5.10",
    "start-server-and-test": "^1.15.5",
    "tailwind-scrollbar": "^2.1.0",
    "tailwindcss": "^3.3.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^3.13.0",
    "typescript": "^5.1.0",
    "vite-tsconfig-paths": "^3.6.0",
    "vitest": "^0.24.5"
  },
  "engines": {
    "node": ">=18"
  },
  "prisma": {
    "schema": "app/database/schema.prisma",
    "seed": "tsx app/database/seed.server.ts"
  }
